# Composite MCP Server - Docker Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# DIFY CONFIGURATION (required if dify server is enabled in config)
# =============================================================================
DIFY_API_KEY=your_dify_api_key_here
DIFY_BASE_URL=https://api.dify.ai/v1
DIFY_CONSOLE_API_KEY=your_dify_console_api_key_here
DIFY_CONSOLE_BASE_URL=https://api.dify.ai

# =============================================================================
# BROWSER CONFIGURATION (optional)
# =============================================================================
BROWSER_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000

# =============================================================================
# VECTORSTORE CONFIGURATION (required if vectorstore is enabled in config)
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here

# =============================================================================
# TRANSPORT CONFIGURATION (defaults set in docker-compose.yml)
# =============================================================================
# TRANSPORT=sse
# HOST=0.0.0.0
# PORT=8000
# ALLOW_ORIGIN=*
